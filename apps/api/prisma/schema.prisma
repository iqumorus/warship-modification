generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  username  String   @unique
  createdAt DateTime @default(now())
  matches   Match[]
}

model Match {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  players   User[]
}
